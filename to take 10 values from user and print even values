#include <stdio.h>

int main() {
    int numbers[10];
    int i;

    // Taking 10 values from user
    printf("Enter 10 integer values:\n");
    for(i = 0; i < 10; i++) {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    // Printing even values
    printf("\nEven numbers are:\n");
    for(i = 0; i < 10; i++) {
        if(numbers[i] % 2 == 0) {
            printf("%d ", numbers[i]);
        }
    }

    printf("\n");
    return 0;
}
