#include<stdio.h>
void main()
{
    int l,b;
    int area,peri;
    printf("Enter length of rectangle :");
    scanf("%d", &l);
    printf("%d", &b);
    scanf("%d", &b);
    area=l*b;
    printf("Area of rectangle :%d\n",area);
    peri=2*(l+b);
    printf("Perimeter of rectangle :%d\n",peri);
    
}
