#include<stdio.h>
void main()
{
int month;
printf("enter month number (1-12):");
scanf("%d", &month);

switch(month)
{
    case 1: case 3: case 5: case 7: case 8: case 10: case 12:
    printf("31 days\n");
    break;
    case 4: case 6: case 9: case 11:
    printf("30 days\n");
    break;
    case 2:("28 or 29 days(february)\n");
    break;
    default:
    printf("invalid month!\n");
}
}
