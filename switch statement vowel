#include<stdio.h>
void main()
{
    char ch;
    printf("enter a charachter: ");
    scanf("%c", &ch);
    switch(ch)
    {
        case 'a':case 'A':
        case 'e':case 'E':
        case 'i':case 'I':
        case 'o':case 'O':
        case 'u':case 'U':
        printf("the charachter '%c' is a vowel,ch");
        break;
        default:
        if(isdigit (ch))
        {
            printf("it is a digit");
            
        }
        else
        {
            printf("it is a consonant");
        }
    }
}
